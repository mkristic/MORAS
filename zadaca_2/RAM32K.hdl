CHIP RAM32K
{
	IN in[16], addr[15], load;
	OUT out[16];
	
	PARTS:
	RAM16K (in = in, load = x, address = addr[0..13], out = out1);
	RAM16K (in = in, load = y, address = addr[0..13], out = out2);
	
	DMux (in = load, sel = addr[14], a = x, b = y);
	
	Mux16 (a = out1, b = out2, sel = addr[14], out = out);
	
	
}